{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6 
    },
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true 
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "jsx-quotes": ["error", "prefer-single"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }] 
    }
}
